{
  "expo": {
    "name": "Happy Inline",
    "slug": "happy-inline",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "happyinline",
    "plugins": [
      "expo-image-picker",
      [
        "onesignal-expo-plugin",
        {
          "mode": "production"
        }
      ]
    ],
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.happyinline.app",
      "buildNumber": "7",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"],
        "NSLocationWhenInUseUsageDescription": "Happy Inline needs your location to help you find nearby service providers and show accurate business locations on the map.",
        "NSCameraUsageDescription": "Happy Inline needs camera access to scan QR codes for quick customer registration with your favorite service provider. For example, scan a business QR code to instantly connect with their booking system.",
        "NSPhotoLibraryUsageDescription": "Happy Inline needs photo library access to let you upload a profile picture or business logo to personalize your account."
      }
    },
    "android": {
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.happyinline.app",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "POST_NOTIFICATIONS",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
